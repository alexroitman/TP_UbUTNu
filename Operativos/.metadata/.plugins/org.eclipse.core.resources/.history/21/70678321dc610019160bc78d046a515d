#include <sock/sockets-lib.h>
#define IP "127.0.0.1"
#define PUERTO "2000"
#include <pthread.h>
int main(){
	printf("hola mundo");
	int socket_servidor = levantarServidor(PUERTO);
	int mi_socket = aceptarCliente(socket_servidor);
	recibirMensaje(mi_socket);
	close(socket_servidor);
	close(mi_socket);
	return 0;
}
