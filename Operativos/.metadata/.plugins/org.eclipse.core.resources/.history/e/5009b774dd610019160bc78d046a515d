#include <sock/sockets-lib.h>
#define IP "127.0.0.1"
#define PUERTOESCUCHA "2000"
#include <pthread.h>
int main(){
	int mi_socket = levantarServidor(PUERTOESCUCHA);
	mi_socket = aceptarCliente(mi_socket);
	recibirMensaje(mi_socket);
	close(mi_socket);
	return 0;
}
